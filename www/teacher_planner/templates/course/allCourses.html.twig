{% extends 'base.html.twig' %}
{% block title %}Cursos{% endblock %}
{% block body %}
    <table>
        {% for course in allCourses %}
            <tr>
                <td>{{ course.id }}</td>
                <td>{{ course.name }}</td>
                <td>{{ course.cicle }}</td>
            </tr>
        {% else %}
            <tr>
                <td>No hay resultados.</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}